# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                               controllers.Prova.home()
POST    /minAttribute                   controllers.Prova.postMinAttribute()
GET     /minAttribute                   controllers.Prova.getMinAttribute()
GET     /meteoTempo                     controllers.Prova.getMeteoTemporale()
POST    /meteoTempo                     controllers.Prova.postMeteoTemporale()
GET     /mediaAnnuale                   controllers.Prova.getMediaAnnuale()
POST    /mediaAnnuale                   controllers.Prova.postMediaAnnuale()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
